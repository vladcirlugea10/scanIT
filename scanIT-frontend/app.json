{
  "expo": {
    "name": "scanIT",
    "slug": "scanit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_nobg.png",
    "scheme": "myapp",
    "deepLinks": [
      "myapp://"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrv07.scanit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mrv07.scanit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_nobg.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_nobg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-router",
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f818c370-1822-4dd8-9e69-9c694911e36d"
      },
      "GOOGLE_CLIENT_ID_ANDROID": "757957068975-q0eju3tghvnjt5if7ebvtg83m0f02qqp.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_IOS": "757957068975-atudkjtnontgijt96t2jop41bap9vdjr.apps.googleusercontent.com"
    },
    "owner": "mrv07"
  }
}
