{
  "expo": {
    "name": "scanIT-frontend",
    "slug": "scanIT-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_nobg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_nobg.png",
      "resizeMode": "contain",
      "backgroundColor": "#DBE4EE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DBE4EE"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_nobg.png"
    },
    "plugins": [
        "expo-router",
        [
          "expo-font",
          {
            "fonts": ["./assets/fonts/Montserrat-VariableFont_wght.ttf"]
          }
        ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
